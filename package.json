  "version": "0.8.0",
  "description": "ReactJS version of Director Responsive Admin Template Free",
  "main": "src/index.js",
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "jest && codecov",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "cross-env NODE_ENV=dev node ./server.hot.reload.js",
    "dev": "rimraf docs/public/assets && cross-env NODE_ENV=dev webpack --display-entrypoints --config webpack.dev.config.js",
    "prod": "rimraf docs/public/assets && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js ",
    "dev-server": "npm run dev && node src/server",
    "prod-server": "npm run prod && node src/server"
  },
  "keywords": [
    "director",
    "admin",
    "template",
    "react",
    "redux",
    "react-redux",
    "ES6",
    "ES2015",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "redux-devtools-extension"
